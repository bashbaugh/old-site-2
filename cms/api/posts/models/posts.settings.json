{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Posts",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "url": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "article",
        "tutorial",
        "video"
      ],
      "default": "article"
    },
    "date": {
      "type": "date"
    },
    "summary": {
      "type": "text"
    }
  }
}
